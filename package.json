{
  "name": "@daveyplate/better-auth-ui",
  "homepage": "https://github.com/daveyplate/better-auth-ui",
  "version": "1.0.26",
  "description": "Plug & play shadcn/ui components for better-auth",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "dev": "tsc-watch --onSuccess \"npx eslint\"",
    "prepublishOnly": "rm -rf dist && tsup --clean --dts"
  },
  "keywords": [
    "typescript",
    "react",
    "better-auth",
    "shadcn",
    "nextjs"
  ],
  "author": "daveycodez",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@iconify/react": ">=5.1.0",
    "@icons-pack/react-simple-icons": "^11.0.1",
    "@radix-ui/react-label": ">=2.1.0",
    "@radix-ui/react-slot": ">=1.1.0",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19",
    "class-variance-authority": ">=0.7.0",
    "clsx": ">=2.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "lucide-react": ">=0.469.0",
    "next": "^15.1.3",
    "react-router": "^7.1.1",
    "tailwind-merge": ">=2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tsc-watch": "^6.2.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "turbo": "^2.3.4",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "@iconify/react": ">=5.1.0",
    "@icons-pack/react-simple-icons": ">=11.0.1",
    "@radix-ui/react-label": ">=2.1.0",
    "@radix-ui/react-slot": ">=1.1.0",
    "better-auth": ">=1.1.4",
    "class-variance-authority": ">=0.7.0",
    "clsx": ">=2.1.0",
    "lucide-react": ">=0.469.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwind-merge": ">=2.6.0",
    "tailwindcss": ">=3.0.0"
  },
  "packageManager": "pnpm@10.2.1",
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3"
  }
}