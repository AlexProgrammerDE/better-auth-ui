{
  "name": "@daveyplate/better-auth-ui",
  "homepage": "https://better-auth-ui.com",
  "version": "1.2.2",
  "description": "Plug & play shadcn/ui components for better-auth",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./tanstack": {
      "import": {
        "types": "./dist/tanstack.d.ts",
        "default": "./dist/tanstack.js"
      },
      "require": {
        "types": "./dist/tanstack.d.cts",
        "default": "./dist/tanstack.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "dev": "tsc-watch --onSuccess \"sh -c 'npx eslint && tsx code-sync.ts'\"",
    "prepublishOnly": "rm -rf dist && tsup --clean --dts"
  },
  "keywords": [
    "typescript",
    "react",
    "better-auth",
    "shadcn",
    "nextjs"
  ],
  "author": "daveycodez",
  "license": "MIT",
  "devDependencies": {
    "@daveyplate/better-auth-tanstack": "^1.2.27",
    "@eslint/eslintrc": "^3.3.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-email/components": "^0.0.33",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tanstack/react-query": "^5.67.1",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "axios": "^1.8.1",
    "better-auth": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "esbuild-plugin-preserve-directives": "^0.0.11",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "ignore": "^7.0.3",
    "lucide-react": "^0.477.0",
    "next": "^15.2.1",
    "react-router": "^7.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tsc-watch": "^6.2.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "ua-parser-js": "^2.0.2"
  },
  "peerDependencies": {
    "@daveyplate/better-auth-tanstack": "^1.2.27",
    "@radix-ui/react-avatar": ">=1.1.0",
    "@radix-ui/react-dialog": ">=1.1.0",
    "@radix-ui/react-dropdown-menu": ">=2.1.0",
    "@radix-ui/react-label": ">=2.1.0",
    "@radix-ui/react-slot": ">=1.1.0",
    "@react-email/components": ">=0.0.33",
    "@tanstack/react-query": ">=5.66.0",
    "better-auth": ">=1.2.0",
    "class-variance-authority": ">=0.7.0",
    "clsx": ">=2.1.0",
    "lucide-react": ">=0.469.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "sonner": ">=1.7.0",
    "tailwind-merge": ">=2.6.0",
    "tailwindcss": ">=3.0.0",
    "ua-parser-js": ">=2.0.0"
  },
  "packageManager": "pnpm@10.5.2"
}
