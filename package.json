{
    "name": "@daveyplate/better-auth-ui",
    "homepage": "https://better-auth-ui.com",
    "version": "1.2.23",
    "description": "Plug & play shadcn/ui components for better-auth",
    "scripts": {
        "build": "tsup --clean --dts",
        "dev": "tsc-watch --onSuccess 'biome check --fix' & nodemon --watch . -e '*' --exec 'tsx code-sync.ts' -q",
        "prepublishOnly": "rm -rf dist && tsup --clean --dts"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/server.d.ts",
                "default": "./dist/server.js"
            },
            "require": {
                "types": "./dist/server.d.cts",
                "default": "./dist/server.cjs"
            }
        },
        "./tanstack": {
            "import": {
                "types": "./dist/tanstack.d.ts",
                "default": "./dist/tanstack.js"
            },
            "require": {
                "types": "./dist/tanstack.d.cts",
                "default": "./dist/tanstack.cjs"
            }
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "keywords": [
        "typescript",
        "react",
        "better-auth",
        "shadcn",
        "nextjs"
    ],
    "author": "daveycodez",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@daveyplate/better-auth-tanstack": "^1.2.30",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.3",
        "@react-email/components": "^0.0.33",
        "@tanstack/react-query": "^5.67.2",
        "@types/node": "^22.13.10",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "better-auth": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^16.4.7",
        "esbuild-plugin-preserve-directives": "^0.0.11",
        "ignore": "^7.0.3",
        "lucide-react": "^0.479.0",
        "next": "^15.2.1",
        "nodemon": "^3.1.9",
        "react-router": "^7.3.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss-animate": "^1.0.7",
        "tsc-watch": "^6.2.1",
        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "turbo": "^2.4.4",
        "typescript": "^5.8.2",
        "ua-parser-js": "^2.0.2"
    },
    "peerDependencies": {
        "@daveyplate/better-auth-tanstack": "^1.2.30",
        "@radix-ui/react-avatar": ">=1.1.0",
        "@radix-ui/react-checkbox": ">=1.1.0",
        "@radix-ui/react-dialog": ">=1.1.0",
        "@radix-ui/react-dropdown-menu": ">=2.1.0",
        "@radix-ui/react-label": ">=2.1.0",
        "@radix-ui/react-slot": ">=1.1.0",
        "@radix-ui/react-tabs": ">=1.1.0",
        "@react-email/components": ">=0.0.33",
        "@tanstack/react-query": ">=5.66.0",
        "better-auth": ">=1.2.0",
        "class-variance-authority": ">=0.7.0",
        "clsx": ">=2.1.0",
        "lucide-react": ">=0.469.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "sonner": ">=1.7.0",
        "tailwind-merge": ">=2.6.0",
        "tailwindcss": ">=3.0.0",
        "ua-parser-js": ">=2.0.0"
    },
    "pnpm": {
        "overrides": {}
    },
    "packageManager": "pnpm@10.6.1"
}
